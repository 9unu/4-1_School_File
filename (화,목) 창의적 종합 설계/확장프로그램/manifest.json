{
    "manifest_version": 3,
    "name": "Review_Box",
    "version": "1.0",
    "description": "상품 설명 이미지 옆 리뷰 띄우는 프로그램",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  